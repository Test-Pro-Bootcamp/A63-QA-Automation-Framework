<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Regression" parallel="methods" thread-count="8">
    <parameter name="BaseURL" value="https://qa.koel.app/"/>
    <test name="Regression" preserve-order="false">
        <classes>
            <class name="LoginTests">
                <methods>
                    <include name="loginValidEmailPassword"> </include>
                    <include name="loginInvalidEmailPassword"> </include>
                    <include name="loginEmptyEmailPassword"> </include>
                    <include name="loginEmailEmptyPassword"> </include>
                </methods>
            </class>
            <class name="PlaylistTests">
                <methods>
                    <include name="createPlaylist"> </include>
                    <include name="deletePlaylist"> </include>
                </methods>
            </class>
            <class name="SongTests">
                <methods>
                    <include name="playSong"> </include>
                </methods>
            </class>
        </classes>
    </test>
</suite>